{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nom de fabrique de données",
			"defaultValue": "df-dfv-dk-euw-01"
		},
		"CosmosDbMongoDbApi1_connectionString": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « connectionString » de « CosmosDbMongoDbApi1 »"
		},
		"dlstrv2dvfdkeuw01_accountKey": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « accountKey » de « dlstrv2dvfdkeuw01 »"
		},
		"CosmosDbMongoDbApi1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dvf"
		},
		"dlstrv2dvfdkeuw01_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stdlvfoncieresdkeuw01.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI pour le compte de stockage contenant des modèles ARM liés"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Jeton SAS pour le compte de stockage contenant des modèles ARM liés"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-dfv-dk-euw-01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CosmosDbMongoDbApi1_connectionString": {
						"value": "[parameters('CosmosDbMongoDbApi1_connectionString')]"
					},
					"dlstrv2dvfdkeuw01_accountKey": {
						"value": "[parameters('dlstrv2dvfdkeuw01_accountKey')]"
					},
					"CosmosDbMongoDbApi1_properties_typeProperties_database": {
						"value": "[parameters('CosmosDbMongoDbApi1_properties_typeProperties_database')]"
					},
					"dlstrv2dvfdkeuw01_properties_typeProperties_url": {
						"value": "[parameters('dlstrv2dvfdkeuw01_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}